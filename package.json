    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-mydatepicker": "^0.11.5",
    "date-fns": "^2.29.2",
    "firebase": "^9.17.1",
    "hammerjs": "^2.0.8",
    "instabug-cordova": "^11.3.0",
    "ionic-selectable": "^4.9.0",
    "ngx-image-cropper": "^6.2.1",
    "ngx-spinner": "^14.0.0",
    "rxjs": "^7.5.6",
    "swiper": "^8.4.4",
